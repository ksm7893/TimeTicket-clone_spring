<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.doit.ik.mapper.JoinusMapper">

	<insert id="joinMem">
	INSERT INTO mem(mem_id,mem_pw,mem_name,mem_mail,mem_num,mem_join,mem_point)
	VALUES (#{mem_id},#{mem_pw},#{mem_name},#{mem_mail},#{mem_num},SYSDATE,0)
	</insert>
    
</mapper>